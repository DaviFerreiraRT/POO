
public class atividadeMetodo {
	public static double somarArray(double x[]) {
		double soma=0;
		for(int i=0;i<x.length;i++) {
			soma+=x[i];
		
		}
		
		return soma/x.length;
	}
	public static void main(String[] args) {
		double[] y = new double[3];
		y[0] = 1;
		y[1] = 10;
		y[2] = 5;
	System.out.println(somarArray(y));
	
	}

}
