import java.util.Scanner;
public class Nome {
	public static void somar(int x,int y) {
		int s= x+y;
		System.out.println("A soma é igual a "+s);
	}
	public static void imprimir(int x ) {
		System.out.println("Parametro recebido: "+x);
	}

	public static void main(String[] args) {
		int a=10;
		System.out.println("Antes de chamar o metodo imprimir");
		imprimir(5);
		System.out.println("Depois de chamar pela primeira vez");
		imprimir(a);
		System.out.println("Depois de chamar pela segunda vez");
		System.out.println("Antes de chamar o método somar");
		somar(10,30);
		int b=15;
		int c=15;
		somar(b,c);
		System.out.println("Depois de chamar o método somar");
}
}
