import java.util.Scanner;
public class Nome {
	public static void somarArray(int x []) {
		x[2]=x[0]+x[1];
	}
	public static int novoSomar(int x,int y) {
		int s= x+y;
		return s;
		}
	public static void somar(int x,int y) {
		int s= x+y;
		imprimir(s);
	}
	public static void imprimir(int x ) {
		System.out.println("Parametro recebido: "+x);
	}

	public static void main(String[] args) {
		int a=10;
		System.out.println("Antes de chamar o metodo imprimir");
		imprimir(5);
		System.out.println("Depois de chamar pela primeira vez");
		imprimir(a);
		System.out.println("Depois de chamar pela segunda vez");
		System.out.println("Antes de chamar o método somar pela primeira vez");
		somar(10,30);
		System.out.println("Antes de chamar o método somar pela segunda vez");
		int b=15;
		int c=25;
		System.out.println("Antes de chamar o método somar pela segunda vez");
		somar(b,c);
		System.out.println("Depois de chamar o método somar pela segunda vez");
		int soma=novoSomar(b,c);
		System.out.println(soma);
		 int[] y =new int[3];
		 y[0]=1;
		 y[1]=10;
		 y[2]=20;
		 System.out.println("Antes "+y[2]);
		 somarArray(y);
		
}
}
