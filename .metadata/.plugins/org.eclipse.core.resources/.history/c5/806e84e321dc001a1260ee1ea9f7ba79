import java.util.Scanner;
public class Desafio {

	public static int fib(int fib) {
		int i,fib1=1,fib2=1,soma;
		for(i=3;i<=n;i++) {
			soma=fib1=fib2;
			fib1=fib2;
			fib2=soma;
		}
		
		return 0;
	}
	public static void main(String[] args) {
		Scanner ler=new Scanner(System.in);
		int n=0;
		n=ler.nextInt();
		int resultado=fib(n);
		System.out.println(resultado);
		
	}

}
