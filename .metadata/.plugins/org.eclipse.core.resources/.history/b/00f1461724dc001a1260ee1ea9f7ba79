import java.util.Scanner;
public class Desafio {

	public static int fib(int recur) {
	
		if(recur==1) {
			return 1;
		}
		else
			if(recur==2) {
				return 1;
			}
			else return fib(recur-1)+fib(recur-2);
	}
	public static void main(String[] args) {
		Scanner ler=new Scanner(System.in);
		int n=0;
		n=ler.nextInt();
		int resultado=fib(n);
		System.out.println("A sequencia de fibonnaci é "+resultado);
		
	}

}
